{
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "memory": 512,
      "routes": [
        {
          "src": "/api/.*",
          "dest": "api/server.js"
        }
      ]
    }
  },
  "headers": [
    {
      "source": "/api/sendAudio",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
